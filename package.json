    "framework7-svelte": "^6.3.16",
    "material-icons": "^1.10.7",
    "skeleton-elements": "^3.5.0",
    "svelte": "^3.46.4",
    "swiper": "^6.8.4"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.37",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^6.7.1",
    "vite": "^2.8.4",
